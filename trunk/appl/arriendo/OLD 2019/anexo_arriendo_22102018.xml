<Report>
	<Font name="Arial" size="7" style="" id="ArialSmall"/>
	<Font name="Arial" size="7" style="" id="ArialSmallBlue" red="0" green="0" blue="128"/>
	<Font name="Arial" size="17" style="B" id="ArialBoldBigB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="10" style="B" id="ArialBold"/>
	<Font name="Arial" size="10" style="" id="Arial"/>
	<Font name="Arial" size="8" style="B" id="ArialBold_header"/>	
	<Font name="Arial" size="10" style="B" id="ArialBoldBlue" red="0" green="0" blue="128"/>
	<PageHeader>	
		<Cell text="" height="55%" newline="1"/>		
		<Cell text="PAG. {SectionPageNumber}/{snb}" align="R" font="ArialSmall" newline="1" />
		<Cell text="ANEXO ARRIENDO Nº {strCOD_ARRIENDO} AL {strFECHA_ACTUAL}" width="100%" align="C" newline="1" font="ArialBoldBigB"/>
		<Cell text="" height="4" newline="1"/>	
	</PageHeader>
	<Content font="Arial">
		<Cell text="{ITEM}" width="10%" align="C" border="LRTB" font="Arial"/>	
		<Cell text="{ST_COD_PRODUCTO}" width="20%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{ST_NOM_PRODUCTO}" width="60%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{ST_CANTIDAD}" width="10%" align="R" border="LRTB" font="Arial" newline="1"/>	
	</Content>
	<Group local="COD_ARRIENDO">
		<GroupHeader>				
			<Cell height="12" newline="1"/>
			<Cell text="Nombre Empresa:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{NOM_EMPRESA}" width="62%" align="L" border="B" font="ArialBold"/>
			<Cell text="Rut:" width="5%" align="L" font="ArialBoldBlue"/>
			<Cell text="{RUT}" width="10%" align="L" format="N0,." border="B" font="Arial"/>
			<Cell text="- {DIG_VERIF}" width="5%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="Dirección:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{DIRECCION}  {NOM_COMUNA}  {NOM_CIUDAD}  {NOM_PAIS}" width="82%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="Referencia:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{REFERENCIA}" width="52%" align="L" border="B" font="Arial"/>
			<Cell text="CECO:" width="7%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{CENTRO_COSTO_CLIENTE}" width="10%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="___________________________________________________________" width="100%" align="L" font="ArialBoldBigB" newline="1"/>
			<Cell text="Nombre Contrato:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{NOM_ARRIENDO}" width="82%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="Ubicación Contrato:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{UBICACION_CONTRATO}" width="82%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="Ejecutivo Contrato:" width="18%" align="L"  font="ArialBoldBlue"/>
			<Cell text="{EJECUTIVO_CONTRATO}" width="82%" align="L" border="B" font="Arial" newline="1"/>
			<Cell text="" height="4" newline="1"/>
			<Cell text="" height="4" newline="1"/>
			<Cell text="" height="5" newline="1"/>		
			<Cell text="Item" width="10%" align="C" border="LRTB" font="ArialBoldBlue"/>		
			<Cell text="Modelo" width="20%" align="C" border="LRTB" font="ArialBoldBlue"/>
			<Cell text="Equipo" width="60%" align="C" border="LRTB" font="ArialBoldBlue"/>		
			<Cell text="Cantidad" width="10%" align="C" border="LRTB" font="ArialBoldBlue" newline="1"/>
		</GroupHeader>
		<GroupFooter>
			<Cell text="" height="30" newline="1"/>
			<Cell text="BIGGI RENTAL" width="30%" align="C" font="ArialBold" newline="1"/>
			<Cell text="{USUARIO}" width="30%" align="C" font="Arial" newline="1"/>
		</GroupFooter> 
	</Group>
</Report>