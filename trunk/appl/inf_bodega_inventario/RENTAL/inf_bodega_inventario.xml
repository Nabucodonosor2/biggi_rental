<Report>
	<Font name="Arial" size="7" style="" id="ArialSmall"/>
	<Font name="Arial" size="17" style="B" id="ArialBoldBigB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="8" style="B" id="ArialBold"/>
	<Font name="Arial" size="8" style="B" id="ArialBold_header"/>	
	<Font name="Arial" size="10" style="" id="ArialSmallBlue" red="0" green="0" blue="128"/>
	<Font name="Arial" size="10" style="B" id="ArialSmallBlueB" red="0" green="0" blue="128"/>
	<Font name="Arial" size="11" style="" id="Arial"/>
	<PageHeader>	
		<Cell text="" height="55%" newline="1"/>		
		<Cell text="PAG. {SectionPageNumber}/{snb}" align="R" font="ArialSmall" newline="1" />
		<Cell text="Inventario Sistema Web Rental" width="100%"  align="C" newline="1" font="ArialBoldBigB"/>
		<Cell text="{str_filtro}" width="100%" align="C" newline="1" font="ArialSmallBlueB"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>
		<Cell text="" height="4" newline="1"/>	
		<Cell text="IT" width="5%" align="C" border="LRTB" font="ArialSmallBlueB"/>		
		<Cell text="MODELO" width="16%" align="C" border="LRTB" font="ArialSmallBlueB"/>		
		<Cell text="EQUIPO" width="72%" align="C" border="LRTB" font="ArialSmallBlueB"/>
		<Cell text="STOCK" width="7%" align="C" border="LRTB" font="ArialSmallBlueB" newline="1"/>
	</PageHeader>
	<Content font="Arial">
		<Cell text="{ROWNUMBER}" width="5%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{COD_PRODUCTO}" width="16%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{NOM_PRODUCTO}" nowrap="yes" width="72%" align="L" border="LRTB" font="Arial"/>	
		<Cell text="{CANTIDAD}" width="7%" align="C" border="LRTB" font="Arial" newline="1"/>	
	</Content>
</Report>